{
  "name": "shopware/fixture-bundle",
  "type": "shopware-bundle",
  "description": "Fixture Bundle for Shopware 6",
  "minimum-stability": "stable",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Shopware\\FixtureBundle\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "shopware/administration": "^6.6",
    "shopware/core": "^6.6",
    "shopware/storefront": "^6.6"
  },
  "extra": {
    "shopware-bundle-name": "FixtureBundle"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
